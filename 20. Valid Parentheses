class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        my_list = {
            ")":"(",
            "]":"[",
            "}":"{"
        }
        current_list = []
        for n in s:
            if  n in my_list:
                if current_list:

                    if my_list[n] == current_list[-1]:
                        current_list.pop()
                    else:
                        return False
                else :
                    return False
                
            else:
                current_list.append(n)
        if not current_list:
            return True
        else:
            return False 
   
