class Solution(object):
    def intToRoman(self, num):
        """
        :type num: int
        :rtype: str
        """
        result = []
        number = {
            1 : "I",
            5 : "V",
            10 : "X",
            50 : "L",
            100 : "C",
            500 : "D",
            1000 : "M"
        }
        while num > 0:
            if num >= 1000:
                num -= 1000
                result.append(number[1000])
                continue
            if num >= 900:
                num -= 900
                result.append(number[100])
                result.append(number[1000])
                continue
            if num >= 500:
                num -= 500
                result.append(number[500])
                continue
            if num >= 400:
                num -= 400
                result.append(number[100])
                result.append(number[500])
                continue
            if num >= 100:
                num -= 100
                result.append(number[100])
                continue
            if num >= 90:
                num -= 90
                result.append(number[10])
                result.append(number[100])
                continue
            if num >= 50:
                num -= 50
                result.append(number[50])
                continue
            if num >= 40:
                num -= 40
                result.append(number[10])
                result.append(number[50])
                continue
            if num >= 10:
                num -= 10
                result.append(number[10])
                continue
            if num == 9:
                
                result.append(number[1])
                result.append(number[10])
                break
            if num >= 5:
                num -= 5
                result.append(number[5])
                continue
            if num == 4:
                result.append(number[1])
                result.append(number[5])
                break
            if num > 0:
                num -= 1
                result.append(number[1])
        return "".join(result)
            

            

        
