class Solution(object):
    def removeNthFromEnd(self, head, n):
        """
        :type head: Optional[ListNode]
        :type n: int
        :rtype: Optional[ListNode]
        """
        dummy = ListNode()
        dummy.next = head
        fast = dummy
        slow = dummy
        for m in range(n + 1):
            if fast is None:
                return head
            fast = fast.next

        while fast is not None:
            
            fast = fast.next
            slow = slow.next
        slow.next = slow.next.next

        return dummy.next
