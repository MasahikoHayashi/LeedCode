class Solution(object):
    def maxArea(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        i = 0
        j = len(height) - 1
        max = 0
        while i < j:
            square = min(height[i],height[j]) * (j - i)
            if square > max :
                max = square
            if height[i] >= height[j]:
                j -= 1
            elif height[i] < height[j]:
                i += 1
        return max
        
