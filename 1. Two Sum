class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        result = []
        for n in range(len(nums)):
            for m in range(len(nums) - (n+1)):
                if nums[n] + nums[n + m + 1] == target:
                    result = [n,n+m+1]
                    break
        return result
